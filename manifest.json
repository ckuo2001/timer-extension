{
  "manifest_version": 3,
  "name": "Timer",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "nativeMessaging", "tabs"
  ],
  "externally_connectable": {
    "ids": [
      "*"
    ],
    "matches": ["*://*.google.com/*"]
  }
}